<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wj.manager.business.mapper.PositionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wj.manager.business.entity.Position">
        <id column="position_id" property="positionId" />
        <result column="name" property="name" />
        <result column="description" property="description" />
        <result column="created_time" property="createdTime" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        position_id, name, description, created_time, status
    </sql>

</mapper>
